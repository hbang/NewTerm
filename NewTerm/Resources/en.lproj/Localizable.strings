<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Generic title displayed before the terminal sets a proper title. -->
	<key>TERMINAL</key>
	<string>Terminal</string>

	<!-- Title of Settings page. -->
	<key>SETTINGS</key>
	<string>Settings</string>

	<!-- Alert title displayed when a terminal could not be launched. -->
	<key>TERMINAL_LAUNCH_FAILED</key>
	<string>Couldn’t start the terminal subprocess because an error occurred.</string>

	<!-- Title displayed when the terminal’s process has ended. -->
	<key>PROCESS_COMPLETED_TITLE</key>
	<string>Process completed</string>

	<!-- Message indicating the user can press any key to restart the terminal. -->
	<key>PROCESS_COMPLETED_MESSAGE</key>
	<string>Press any key to close the tab.</string>
</dict>
</plist>
